{
    "extends": "airbnb-base",
    "rules": {
      "indent": ["error", 2],
      "no-param-reassign": "error",
      "max-len": ["error", 152],
      "import/no-dynamic-require": 0,
      "no-underscore-dangle": ["error", {
        "allow": [
          "__base"
        ]
      }],
      "strict": 0,
      "comma-dangle": ["error", "never"]
    }
}
